$n='GitKraken';$t="$env:TEMP\$n";$exe="$t\GitKrakenSetup.exe";New-Item -ItemType Directory -Path $t -Force|Out-Null;Write-Host "Downloading $n..." -ForegroundColor Cyan;Invoke-WebRequest -Uri 'https://api.gitkraken.dev/releases/production/windows/x64/active/GitKrakenSetup.exe' -OutFile $exe;Write-Host "Installing $n..." -ForegroundColor Green;Start-Process $exe -Wait;Write-Host "DONE: $n installed" -ForegroundColor Green
